<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawerlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <FrameLayout
        android:id="@+id/fragment_layout"
        android:layout_width="match_parent"

        android:layout_height="match_parent" >

        <RelativeLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context="com.github.hitgif.powerscore.MainActivity"
            android:fitsSystemWindows="true"
            android:background="#ffffff"
            android:visibility="visible">

            <RelativeLayout
                android:id="@+id/relativeLayout3"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@color/main">


                <ImageView
                    android:id="@+id/sync"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:src="@drawable/sync_butt"
                    android:background="#00000000"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:padding="13dp" />

                <ImageView
                    android:id="@+id/personal"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:src="@drawable/leftdraw_butt"
                    android:background="#00000000"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:padding="9dp" />

                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="New Button"
                    android:id="@+id/ls"
                    android:textColor="#000000"

                    android:layout_below="@+id/pnm"
                    android:layout_toLeftOf="@+id/sync"
                    android:layout_toStartOf="@+id/sync"></Button>


                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/personal"
                    android:id="@+id/pcr">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="未选择"
                        android:id="@+id/classnow"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true"
                        android:textColor="#ffffff"
                        android:textSize="18dp" />

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:id="@+id/dropclass"
                        android:layout_toRightOf="@+id/classnow"
                        android:src="@drawable/drop"
                        android:layout_marginLeft="5dp"
                        android:layout_alignBottom="@+id/classnow"
                        android:paddingBottom="3dp" />

                    <Button
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/pickclass"
                        android:layout_weight="1"
                        android:background="#00000000"
                        android:visibility="visible"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignRight="@+id/dropclass"
                        android:layout_alignParentBottom="true" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/personal"
                    android:id="@+id/pnm"
                    android:visibility="gone">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:id="@+id/nmdr"
                        android:layout_toRightOf="@+id/_name"
                        android:src="@drawable/drop"
                        android:layout_marginLeft="5dp"
                        android:layout_alignBottom="@+id/_name"
                        android:paddingBottom="3dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="未选择"
                        android:id="@+id/_name"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true"
                        android:textColor="#ffffff"
                        android:textSize="18dp"
                        android:visibility="visible" />

                    <Button
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/pnmb"
                        android:background="#00000000"
                        android:visibility="visible"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_toStartOf="@+id/nmdr"
                        android:layout_alignRight="@+id/nmdr" />
                </RelativeLayout>


            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:id="@+id/upt"
                android:layout_alignTop="@+id/relativeLayout5"
                android:background="@color/main">

                <Button
                    android:layout_width="90dp"
                    android:layout_height="43dp"
                    android:layout_weight="1"
                    android:background="@color/main"
                    android:text="全局"
                    android:textSize="15dp"
                    android:id="@+id/gen"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:textColor="#ffffff" />

                <Button
                    android:layout_width="90dp"
                    android:layout_height="43dp"
                    android:layout_weight="1"
                    android:background="@color/main"
                    android:textSize="15dp"
                    android:text="个人"
                    android:textColor="#7fffffff"
                    android:id="@+id/per"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/gen"
                    android:layout_toEndOf="@+id/gen" />

                <RelativeLayout
                    android:orientation="horizontal"
                    android:layout_width="180dp"
                    android:layout_height="3dp"
                    android:layout_alignParentBottom="true"
                    android:id="@+id/lpd">

                    <ImageView
                        android:layout_width="90dp"
                        android:layout_height="3dp"
                        android:id="@+id/bar"
                        android:background="#ffffff"
                        android:layout_alignParentBottom="true" />

                </RelativeLayout>

                <RelativeLayout
                    android:orientation="horizontal"
                    android:layout_width="180dp"
                    android:layout_height="3dp"
                    android:layout_alignParentBottom="true"
                    android:id="@+id/lpd2"
                    android:visibility="gone"
                    android:gravity="right">

                    <ImageView
                        android:layout_width="90dp"
                        android:layout_height="3dp"
                        android:id="@+id/imageView41"
                        android:background="#ffffff"
                        android:layout_alignParentBottom="true" />
                </RelativeLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:paddingRight="13dp"
                    android:background="@drawable/back_press"
                    android:id="@+id/flit">

                    <ImageView
                        android:id="@+id/fb"
                        android:layout_width="30dp"
                        android:layout_height="50dp"
                        android:src="@drawable/thun"
                        android:background="#00000000"
                        android:layout_alignBottom="@+id/bar"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:paddingTop="11dp"
                        android:paddingBottom="11dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="筛选器"
                        android:id="@+id/textView31"
                        android:textColor="#ffffff"
                        android:textSize="15dp" />
                </LinearLayout>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/press"
                android:paddingLeft="10dp"
                android:layout_below="@+id/relativeLayout3"
                android:layout_alignLeft="@+id/relativeLayout3"
                android:id="@+id/relativeLayout5">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:id="@+id/fliter"
                    android:visibility="gone"
                    android:paddingTop="10dp"
                    android:paddingBottom="5dp">

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentLeft="true"
                        android:id="@+id/relativeLayout"
                        android:paddingRight="10dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="时间"
                            android:id="@+id/textView2"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentLeft="true"
                            android:textColor="#e1e1e1" />

                    </RelativeLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView2"
                        android:src="@drawable/jg"
                        android:layout_alignParentBottom="true"
                        android:layout_toRightOf="@+id/relativeLayout"
                        android:paddingBottom="5dp" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView4"
                        android:src="@drawable/jg"
                        android:paddingBottom="5dp"
                        android:layout_toRightOf="@+id/imageView2" />

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:id="@+id/relativeLayout4"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_alignParentBottom="true"
                        android:layout_toRightOf="@+id/imageView4">

                    </RelativeLayout>
                </RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/genlayout"
                android:visibility="gone"
                android:layout_below="@+id/upt"
                android:layout_alignParentBottom="true">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="暂无记录 :("
                    android:layout_weight="0"
                    android:id="@+id/textView13"
                    android:layout_alignParentTop="true"
                    android:gravity="center_vertical|center_horizontal"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:textColor="#6e6e6e" />
                <ListView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/listView3"
                    android:layout_weight="1"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />


            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:id="@+id/perlayout"
                android:visibility="visible"
                android:layout_below="@+id/upt">

            </RelativeLayout>

            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:id="@+id/add"
                android:layout_weight="3"
                android:src="@drawable/add"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="50dp"
                android:layout_marginRight="30dp"
                android:layout_alignParentBottom="true"
                android:background="@drawable/add_butt" />

        </RelativeLayout>

    </FrameLayout>

    <RelativeLayout
        android:id="@+id/left"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:background="@android:color/white">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:id="@+id/ds"></LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_alignParentStart="true"
            android:background="@color/main"
            android:id="@+id/relativeLayout18"
            android:layout_below="@+id/ds"
            android:paddingTop="20dp">

            <ImageView
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:id="@+id/imageView"
                android:src="@drawable/setting_tx"

                android:layout_marginBottom="7dp"
                android:layout_marginLeft="15dp"
                android:layout_alignParentTop="true"
                />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="realname"
                android:id="@+id/realname"
                android:layout_marginBottom="15dp"
                android:layout_alignParentBottom="false"
                android:textColor="#ffffff"
                android:textSize="20dp"
                android:paddingLeft="18dp"
                android:layout_below="@+id/imageView" />

            <ImageView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="18dp"
                android:layout_marginRight="15dp"
                android:layout_alignParentBottom="false"
                android:id="@+id/home"
                android:src="@drawable/bome_btn"
                android:layout_alignParentTop="true"
                android:layout_marginTop="7dp" />


        </RelativeLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/scrollView"
            android:background="@color/light_gray"
            android:layout_below="@+id/relativeLayout18"
            android:layout_above="@+id/buo">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/ses"
                    android:layout_below="@+id/relativeLayout18">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/relativeLayout20"
                        android:visibility="visible">

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"

                            android:background="#eff1f2">

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="5dp"></LinearLayout>



                        </LinearLayout>

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/relativeLayout20"
                        android:id="@+id/RL"
                        android:paddingTop="1dp">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="50dp"

                            android:background="@drawable/button_press"
                            android:id="@+id/overView"
                            android:paddingLeft="10dp"
                            android:gravity="center_vertical"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true">

                            <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:id="@+id/imageView35"
                                android:src="@drawable/stop"
                                android:paddingTop="8dp"
                                android:paddingBottom="8dp" />

                            <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:text="学分总览"
                            android:id="@+id/textView25"
                            android:textColor="#3b454b"
                            android:paddingLeft="10dp"
                            android:textSize="15dp" />
                        </LinearLayout>
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/relativeLayout21"
                        android:layout_below="@+id/RL"
                        android:layout_alignParentStart="true"
                        android:paddingTop="1dp"
                        android:visibility="visible">

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="wrap_content"
                                    android:layout_height="50dp"
                                    android:layout_below="@+id/relativeLayout20"
                                    android:background="@drawable/button_press"
                                    android:id="@+id/reason"
                                    android:paddingLeft="10dp"
                                    android:gravity="center_vertical"
                                    android:layout_alignParentRight="true"
                                    android:layout_alignParentEnd="true"
                                    android:layout_alignParentLeft="true"
                                    android:layout_alignParentStart="true">

                                    <ImageView
                                        android:layout_width="40dp"
                                        android:layout_height="40dp"
                                        android:id="@+id/imageView37"
                                        android:src="@drawable/reason"
                                        android:paddingTop="8dp"
                                        android:paddingBottom="8dp" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textAppearance="?android:attr/textAppearanceLarge"
                                        android:text="常用理由"
                                        android:id="@+id/textView27"
                                        android:textColor="#3b454b"
                                        android:paddingLeft="10dp"
                                        android:textSize="15dp" />
                                </LinearLayout>

                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:paddingTop="1dp">

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="wrap_content"
                                    android:layout_height="50dp"
                                    android:background="@drawable/button_press"
                                    android:id="@+id/linearLayout7"
                                    android:layout_below="@+id/imageView38"
                                    android:paddingLeft="10dp"
                                    android:gravity="center_vertical"
                                    android:layout_alignParentRight="true"
                                    android:layout_alignParentEnd="true"
                                    android:layout_alignParentLeft="true"
                                    android:layout_alignParentStart="true">

                                    <ImageView
                                        android:layout_width="40dp"
                                        android:layout_height="40dp"
                                        android:id="@+id/imageView39"
                                        android:src="@drawable/group"
                                        android:paddingTop="8dp"
                                        android:paddingBottom="8dp" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textAppearance="?android:attr/textAppearanceLarge"
                                        android:text="成员组"
                                        android:id="@+id/textView28"
                                        android:textColor="#3b454b"
                                        android:paddingLeft="10dp"
                                        android:textSize="15dp" />
                                </LinearLayout>

                            </RelativeLayout>

                        </LinearLayout>
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/relativeLayout17"
                        android:paddingTop="1dp"
                        android:layout_below="@+id/relativeLayout21"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true">

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:id="@+id/setspl"
                            android:background="@drawable/back_press" >

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/button_press">

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="wrap_content"
                                    android:layout_height="50dp"
                                    android:layout_below="@+id/relativeLayout20"
                                    android:id="@+id/linearLayout8"
                                    android:paddingLeft="10dp"
                                    android:gravity="center_vertical" >

                                    <ImageView
                                        android:layout_width="40dp"
                                        android:layout_height="40dp"
                                        android:id="@+id/imageView42"
                                        android:src="@drawable/window"
                                        android:paddingTop="8dp"
                                        android:paddingBottom="8dp" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textAppearance="?android:attr/textAppearanceLarge"
                                        android:text="显示启动页"
                                        android:id="@+id/textView30"
                                        android:textColor="#3b454b"
                                        android:paddingLeft="10dp"
                                        android:textSize="15dp" />


                                </LinearLayout>
                                <ImageView
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:id="@+id/onspl"
                                    android:src="@drawable/btick"
                                    android:paddingTop="8dp"
                                    android:paddingBottom="8dp"
                                    android:layout_marginRight="10dp"
                                    android:layout_alignParentRight="true"
                                    android:layout_centerVertical="true" />

                            </RelativeLayout>

                        </LinearLayout>
                    </RelativeLayout>

                </RelativeLayout>

            </RelativeLayout>
        </ScrollView>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_alignParentBottom="true"
            android:background="#ffffff"
            android:id="@+id/buo">

            <Button
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_weight="1"
                android:text="关于"
                android:textSize="15dp"
                android:id="@+id/buttong"
                android:background="@drawable/white_button_press"
                android:textColor="#3b454b"
                android:visibility="visible"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true"
                android:layout_toLeftOf="@+id/imageView34"
                android:layout_toStartOf="@+id/imageView34" />

            <ImageView
                android:layout_width="1dp"
                android:layout_height="30dp"
                android:id="@+id/imageView34"
                android:layout_weight="1"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:background="@color/light_gray" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_weight="1"
                android:text="退出登录"
                android:textSize="15dp"
                android:id="@+id/button2"
                android:background="@drawable/white_button_press"
                android:textColor="#f32020"
                android:visibility="visible"
                android:layout_alignParentBottom="true"
                android:layout_toRightOf="@+id/imageView34"
                android:layout_toEndOf="@+id/imageView34" />
        </RelativeLayout>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/right"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:background="@android:color/white">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:id="@+id/ds2" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/main"
            android:paddingLeft="10dp"
            android:id="@+id/relativeLayout27"
            android:layout_gravity="right|top"
            android:layout_below="@+id/ds2"
            android:paddingRight="10dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/relativeLayout26"
                android:visibility="visible"
                android:paddingBottom="5dp"
                android:paddingTop="5dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="80dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:id="@+id/relativeLayout19"
                    android:paddingRight="10dp"
                    android:paddingLeft="10dp"
                    android:gravity="center_vertical">

                    <ImageView
                        android:layout_width="10dp"
                        android:layout_height="20dp"
                        android:id="@+id/droppp"
                        android:src="@drawable/drop"
                        android:layout_toRightOf="@+id/textView7"
                        android:paddingBottom="7dp"
                        android:layout_alignBottom="@+id/pick" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="时间"
                        android:id="@+id/textView29"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentLeft="true"
                        android:textColor="#8cffffff" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:id="@+id/year"
                        android:textColor="#ffffff"
                        android:layout_alignParentTop="true"
                        android:layout_alignRight="@+id/droppp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="50dp"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:id="@+id/month"
                        android:textColor="#ffffff"
                        android:text="不限"
                        android:gravity="center_vertical"
                        android:layout_below="@+id/textView29"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignBottom="@+id/pick" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:id="@+id/textView5"
                        android:textColor="#ffffff"
                        android:layout_toRightOf="@+id/month"
                        android:layout_alignBottom="@+id/pick" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:id="@+id/day"
                        android:textColor="#ffffff"
                        android:gravity="center_vertical"
                        android:layout_toRightOf="@+id/textView5"
                        android:layout_alignTop="@+id/month"
                        android:layout_alignBottom="@+id/month" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:id="@+id/textView7"
                        android:textColor="#ffffff"
                        android:layout_toRightOf="@+id/day"
                        android:layout_alignBottom="@+id/pick" />

                    <Button
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/pick"
                        android:layout_weight="1"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignRight="@+id/year"
                        android:background="#00000000"
                        android:visibility="visible" />
                </RelativeLayout>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView43"
                    android:layout_below="@+id/relativeLayout19"
                    android:src="@drawable/jgdh" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="80dp"
                    android:id="@+id/relativeLayout24"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:layout_below="@+id/imageView43"
                    android:gravity="center_vertical">

                    <ImageView
                        android:layout_width="10dp"
                        android:layout_height="20dp"
                        android:id="@+id/drop2"
                        android:src="@drawable/drop"
                        android:paddingBottom="7dp"
                        android:layout_alignBottom="@+id/pickpm"
                        android:layout_toRightOf="@+id/pm" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="加/减分"
                        android:id="@+id/textView36"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentLeft="true"
                        android:textColor="#8cffffff" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:id="@+id/pm"
                        android:textColor="#ffffff"
                        android:textIsSelectable="false"
                        android:text="不限"
                        android:layout_alignParentLeft="true"
                        android:gravity="center_vertical"
                        android:layout_below="@+id/textView36"
                        android:layout_alignBottom="@+id/pickpm" />

                    <Button
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/pickpm"
                        android:layout_weight="1"
                        android:background="#00000000"
                        android:layout_alignParentTop="true"
                        android:layout_alignLeft="@+id/textView36"
                        android:layout_alignStart="@+id/textView36" />
                </RelativeLayout>

            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>

</android.support.v4.widget.DrawerLayout>
